   "source": [
    "input_list=input()\n",
    "k=input_list.split(\",\")\n",
    "c1,c2=0,0\n",
    "for i in k:\n",
    "    if(int(i)%2==0):\n",
    "        c1+=1\n",
    "    else:\n",
    "        c2+=1\n",
    "print(\"Number of even numbers :\",c1)\n",
    "print(\"Number of odd numbers :\",c2)"
   ]
