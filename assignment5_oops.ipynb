    "class Power:\n",
    "    def __init__(self,x,n):\n",
    "        self.x=x\n",
    "        self.n=n\n",
    "    def pow(self,x,n):\n",
    "        k=1\n",
    "        for i in range(0,self.n):\n",
    "            k=k*self.x\n",
    "        return(k)\n",
    "x=int(input(\"x\"))\n",
    "n=int(input(\"n\"))\n",
    "p1=Power(x,n)\n",
    "print(p1.pow(x,n))\n",
